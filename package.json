{
  "name": "typeorm-seeding-generator",
  "version": "1.0.3",
  "main": "lib/cli.js",
  "repository": "https://github.com/omarberrami/typeorm-seeding-generator.git",
  "author": "Omar Berrami",
  "license": "MIT",
  "bin": {
    "typeorm-seeding-generator": "./lib/cli.js"
  },
  "scripts": {
    "clean": "rimraf lib/",
    "copy-files": "copyfiles -u 1 src/template/* lib/",
    "build": "yarn clean && tsc -p . && yarn copy-files ",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn run lint",
    "preversion": "yarn run lint",
    "version": "yarn run format && git add -A src",
    "postversion": "git push && git push --tags",
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/cli.ts",
    "seed:generate": "yarn build && ts-node ./lib/cli.js -n"
  },
  "devDependencies": {
    "@types/clear": "^0.1.2",
    "@types/figlet": "^1.5.4",
    "@types/jest": "^27.0.1",
    "jest": "^27.1.1",
    "prettier": "^2.4.0",
    "ts-jest": "^27.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.2"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@types/node": "^16.9.1",
    "chalk": "^4.1.2",
    "clear": "^0.1.0",
    "commander": "^8.2.0",
    "copyfiles": "^2.4.1",
    "figlet": "^1.5.2",
    "nodemon": "^2.0.12",
    "path": "^0.12.7",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "typeorm": "^0.2.37"
  }
}
